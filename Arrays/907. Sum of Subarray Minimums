class Solution {
    public int sumSubarrayMins(int[] arr) {
        int sum = 0;
        for(int i=0;i<arr.length;i++){
            int minValue = arr[i];
            for(int j=i;j<arr.length;j++){
              minValue = Math.min(minValue,arr[j]);
              sum = sum+minValue;
            }
        }
        return sum;
    }
}
